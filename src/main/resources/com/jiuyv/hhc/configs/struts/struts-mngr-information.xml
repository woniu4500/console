<struts>
	<!-- 栏目管理 -->	
	<package name="cmArtCat.qry" extends="mngr-query-default">
		<action name="cmArtCatSetup" class="cmArtCatAction">
			<result name="success">page/information/cm_art_cat_mngr.jsp</result>
		</action>
		<action name="doCheckCmArtCatSetup" class="cmArtCatAction" method="doCheckPage" >
			<result name="success">page/information/cm_art_cat_check.jsp</result>
